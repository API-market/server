{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node bin/www",
    "dev": "nodemon --ext js,ejs --watch ./ --exec 'node bin/www -r dotenv/config'",
    "test": "NODE_ENV=test SQLITE_FILENAME=$(pwd)/test_database.sqlite mocha tests/runner.js",
    "migrate": "sequelize db:migrate",
    "migrate:rollback": "sequelize db:migrate:undo",
    "model:generate": "sequelize model:generate --name",
    "migration:generate": "sequelize migration:generate --name",
    "seed:generate": "sequelize seed:generate --name",
    "seed": "sequelize db:seed:all",
    "seed:rollback": "sequelize db:seed:undo",
    "prestart": "npm run migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lumeos/server.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "atob": "^2.1.1",
    "aws-sdk": "^2.316.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "ejs-locals": "^1.0.2",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.2.0",
    "fcm-push": "^1.1.3",
    "lodash": "^4.17.10",
    "lumeos_middlewares": "file:./middlewares",
    "lumeos_services": "file:./services",
    "lumeos_utils": "file:./utils",
    "multer": "^1.3.1",
    "node-schedule": "^1.3.0",
    "nodemailer": "^4.6.8",
    "pg": "^7.4.3",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "sharp": "^0.20.8",
    "sqlite3": "^4.0.1",
    "translitit-cyrillic-russian-to-latin": "^0.1.1"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "newman": "^3.10.0",
    "nodemon": "^1.18.4",
    "supertest": ""
  }
}
